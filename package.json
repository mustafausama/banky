{
  "name": "banky",
  "version": "1.0.0",
  "description": "Banking system",
  "main": "index.js",
  "scripts": {
    "prettier:check": "prettier --check \"src/**/*.js\"",
    "prettier:fix": "prettier --list-different --write \"src/**/*.js\"",
    "lint:check": "eslint \"src/**/*.js\"",
    "lint:fix-dry-run": "eslint --fix-dry-run \"src/**/*.js\"",
    "lint:fix": "eslint --fix \"src/**/*.js\"",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "npm run format && jasmine",
    "dev": "concurrently \"npm run prisma:generate && nodemon src/index.js\" \"npm run dev:frontend\"",
    "start": "npm run prisma:generate && node src/index.js",
    "dev:frontend": "npm run --prefix views start",
    "build:frontend": "npm run --prefix views build",
    "postinstall": "([ -d \"views\" ] && npm --prefix views install || exit 0) && ([ -f .git/hooks/pre-commit ] || [ -f .pre-commit ] && chmod +x .pre-commit && cp .pre-commit .git/hooks/pre-commit || exit 0)",
    "prisma:generate": "prisma generate --schema=src/models/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/models/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/models/schema.prisma",
    "prisma:push": "prisma db push --schema=src/models/schema.prisma",
    "prisma:format": "prisma format --schema=src/models/schema.prisma",
    "prisma:validate": "prisma validate --schema=src/models/schema.prisma",
    "prisma:seed": "prisma db seed --schema=src/models/schema.prisma"
  },
  "prisma": {
    "seed": "node src/models/seed.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-list-endpoints": "^6.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  }
}
